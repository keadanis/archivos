<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Campaigns.
    -->
    <changeSet id="20200107211433-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="campaigns_id"
                                 baseTableName="campaigns_branch"
                                 constraintName="fk_campaigns_branch_campaigns_id"
                                 referencedColumnNames="id"
                                 referencedTableName="campaigns"/>

        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="campaigns_branch"
                                 constraintName="fk_campaigns_branch_branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branches"/>
        
    </changeSet>
</databaseChangeLog>
