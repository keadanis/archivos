<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Branches.
    -->
    <changeSet id="20200107211430-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="branches_id"
                                 baseTableName="branches_user"
                                 constraintName="fk_branches_user_branches_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branches"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="branches_user"
                                 constraintName="fk_branches_user_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
        
    </changeSet>
</databaseChangeLog>
